// @hulo:build batch;vbs;powershell

// echo [OPTIONS | CMDS]
cmd echo {
    // Display each line of the batch on screen (default).
    on: Command

    // Only display the command output on screen.
    off: Command

    // -h, --help
    @Serialize("/?")
    help: bool

    // a string of characters to display.
    message: bool | str | num

    echo($this.help)
}

fn test() {
    echo off;
    echo -h;
    echo "Hello World";
    echo true;
}